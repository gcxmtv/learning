public class testeVeF {

	
	public static void main(String[] args) {
		
		boolean numero1 = true;
		boolean numero2 = true;
		boolean numero3 = false;
		
		if ( numero1 = true & numero2 == false || numero3 == true ) { System.err.println("é verdadeiro"); }
	else { System.out.println("nao deu certo"); }
	
}}
